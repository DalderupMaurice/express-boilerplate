{
  "env": {
    "node": true,
    "jest": true,
    "mocha": true
  },
  "plugins": [
    "prettier",
  ],
  "extends": [
    "airbnb-base",
    "prettier",
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true }],
    "no-underscore-dangle": "off"
  }
}
